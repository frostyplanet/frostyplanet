--langdef=Rust
--langmap=Rust:.rs
--regex-Rust=/^\s*(#\[[^\]]\]\s*)*(pub\s*(\(crate\))?\s+)?(extern\s+)?("[^"]+"\s+)?(unsafe\s+)?(async\s+)?fn\s+([a-zA-Z0-9_]+)/\8/f,functions,function definitions/
--regex-Rust=/^\s*(pub\s*(\(crate\))?\s+)?type\s+([a-zA-Z0-9_]+)/\3/T,types,type definitions/
--regex-Rust=/^\s*(pub\s*(\(crate\))?\s+)?enum\s+([a-zA-Z0-9_]+)/\3/g,enum,enumeration names/
--regex-Rust=/^\s*(pub\s*(\(crate\))?\s+)?struct\s+([a-zA-Z0-9_]+)/\3/s,structure names/
--regex-Rust=/^\s*(pub\s*(\(crate\))?\s+)?mod\s+([a-zA-Z0-9_]+)/\3/m,modules,module names/
--regex-Rust=/^\s*(pub\s*(\(crate\))?\s+)?(static|const)\s+(mut\s+)?([a-zA-Z0-9_]+)/\5/c,consts,static constants/
--regex-Rust=/^\s*(pub\s*(\(crate\))?\s]+)?(unsafe\s+)?trait\s+([a-zA-Z0-9_]+)/\4/t,traits,traits/
--regex-Rust=/^\s*macro_rules!\s+([a-zA-Z0-9_]+)/\1/d,macros,macro definitions/
